{
  "installCommand": "yarn",
  "buildCommand": "yarn build",
  "git": {
    "deploymentEnabled": {
      "gh-pages": false
    }
  },
  "github": {
    "autoJobCancelation": false
  },
  "headers": [
    {
      "source": "(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=31536000, max-age=31536000"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "max-age=31536000"
        }
      ]
    }
  ]
}
